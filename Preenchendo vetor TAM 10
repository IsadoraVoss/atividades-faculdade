#include <stdio.h>
#include <locale.h> //essa biblioteca é utilizada para manipular configurações de localização;

int main()
{
 setlocale(LC_ALL, "Portuguese");//é para poder imprimir acentuação
 
 int numeros[10], i;
 
 for (i = 0; i< 10; i++){
     printf("Posição %d do vetor:", i);
     scanf("%d", &numeros[i]);
     
 }
 printf("\n\n");
 
 for(i =0; i< 10; i++){
     printf("%d\t", numeros[i]);
 }
 return 0;
}
