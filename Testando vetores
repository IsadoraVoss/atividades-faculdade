#include <stdio.h>

int main()
{
 int vetor[6];
 vetor[0] = 20;
 vetor[1] = 30;
 vetor[2] = 40;
 vetor[3] = 50;
 vetor[4] = 60;
 vetor[5] = 70;
 
 vetor [6] = 80; //isso não existe
 
 printf("Valores do vetor:\n");
 for(int i = 0; i<6; i++) //índice do vetor precisa ser menor que 6 e sempre adicionar mais um;
 {
     printf("vetor [%d] = %d\n", i, vetor[i]);
 }
 return 0;
}
